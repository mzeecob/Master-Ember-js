<h1>Contacts</h1>

<table class="table table-bordered table-striped">
    <thead>
    <tr>
        <th>ID</th>
        <th>E-mail</th>
        <th>Action</th>
    </tr>
    </thead>
    <tbody>
    {{#each model as |contact|}}
        <tr>
            <td>{{contact.id}}</td>
            <td>{{contact.email}}</td>
            <td><button class="btn btn-danger btn-xs" {{action 'deleteLibrary' contact}}>Delete</button></td>
        </tr>
    {{/each}}
    </tbody>
</table>
